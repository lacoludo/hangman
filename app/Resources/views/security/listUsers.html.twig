{% extends 'layout.html.twig' %}

{% block title %}
    {{ parent() }} User list
{% endblock %}

{% block content %}
    <h1>All users registered</h1>

    {% if users %}
    <ul>
        {% for user in users %}
        <ol>{{ loop.index }} - {{ user.fullname }}</ol>
        {% endfor %}
    </ul>
    {% else %}
        No user registered.
    {% endif %}
{% endblock %}
